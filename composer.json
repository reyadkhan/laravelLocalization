{
    "name": "webapp/localization",
    "description": "Simple laravel localization package",
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "WebApp\\Localization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebApp\\Localization\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebApp\\Localization\\LocalizationServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Reyad Khan",
            "email": "reyadk2@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "orchestra/testbench": "^6.0"
    }
}
